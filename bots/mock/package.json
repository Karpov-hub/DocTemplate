    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^7.7.2"
  }
